page {
  height: 100%;
}

.container {
  padding: 2px;
}

.searchInput {
  border: #000 thin solid;
  color: red;
  border-radius: 40px;
  padding-left: 10px;
  font-size: 12px;
  width: 95%;
  margin-bottom: 15px;
}

.item {
  width: 49.5%;
  float: left;
  height: 150px;
  display: flex;
  align-items: center;
  justify-content:center;
}

.itemContent {
  width: 70%;
  height: 60%;
}

.itemImg,.itemImg image{
  height: 95%;
  width:100%;
}

.itemTitle{
  width: 100%;
  font-size: 16px;
  text-align: center;
}